// Online C compiler to run C program online
#include <stdio.h>

int main() {
    char a='A',b='A';
    
    for(int i=1; i<=5; i++){
        b=a+5-i;
        for(int j=5; j>=1; j--){
            printf("%3c", b++);
            b=b+4;
        }
        printf("\n");
    }
    return 0;
}



