// Online C compiler to run C program online
#include <stdio.h>

int main() {
    int a=1;
    
    for(int i=1; i<=5; i++){
        a=i;
        for(int j=1; j<5; j++){
          
            printf("%4d", a);
              a=a+5;
        }
        printf("\n");
    }

    return 0;
}


