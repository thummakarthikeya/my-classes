// Online C compiler to run C program online
#include <stdio.h>

int main() {
    int a=0;
    
    for(int i=1; i<=5; i++){
        
        if(i%2!=0){
            for(int j=1; j<=5; j++){
                printf("%4d", ++a);
              
            }
        }else{
            for(int j=5; j>=1; j--){
                printf("%4d", a--);
            }
        }
        printf("\n");
        a=a+5;
    }

    return 0;
}


