#include <stdio.h>

int main() {
   int n=5,i,j;
    for(i = 1;i<=n;i++) {  
        for(j = 1;j<=n;j++) { 
            printf("%3d",(i+j)%2);  
        }                      
        printf("\n");
    }

    return 0;
}

