// Online C compiler to run C program online
#include <stdio.h>

int main() {
    char a='A',b='A';
    
    for(int i=1; i<=5; i++){
        
        for(int j=1; j<=5; j++){
            printf("%3c", b++);
        }
        printf("\n");
        b=a+i;
    }

    return 0;
}



