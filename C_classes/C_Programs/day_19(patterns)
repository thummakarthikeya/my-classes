// Online C compiler to run C program online
#include <stdio.h>

int main() {
    int a=2;
    for(int i=1; i<=5; i++){
        for(int j=5; j>=1; j--){
            printf("%4d", j);
        }
        printf("\n");
    }

    return 0;
}



