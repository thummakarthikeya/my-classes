// Online C compiler to run C program online
#include <stdio.h>

int main() {
    int a=1;
    for(int i=1; i<=5; i++){
        a=1;
        for(int j=1; j<=6/2; j++){
            printf("%3d%3d",j,i);
        }
        
        printf("\n");
    }

    return 0;
}


