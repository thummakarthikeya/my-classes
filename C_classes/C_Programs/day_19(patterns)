// Online C compiler to run C program online
#include <stdio.h>

int main() {
      int a=5;

      for(int i=a; i>=1; i--){
            int k=i;

        for(int j=1; j<=5; j++){
            printf("%3d",k);
            k=k+5;
        }
        printf("\n");
    }
    return 0;
}
