// Online C compiler to run C program online
#include <stdio.h>

int main() {
    int n=5;
    char ch = 'A';
    for(int i = 1;i<=n;i++) { //i = 1
       
        for(int j = 1;j<=n-1;j++) {
            printf("%3c",ch++);
         
        }
       printf("\n"); 
    }

    return 0;
}
