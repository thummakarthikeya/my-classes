// Online C compiler to run C program online
#include <stdio.h>

int main() {
    int n=5;
    for(int i = 1;i<=n;i++) { 
        int k = 2;
        for(int j = 1;j<=n;j++) { //1 
            if(j<=i) {
                printf("%3d",k*j);
               
            } else {
                printf("%3c",' ');
            }         
        }
      printf("\n"); 
    }  
    return 0;
}
