// Online C compiler to run C program online
#include <stdio.h>

int main() {
    int n=5;
    for(int i = 1;i<=n;i++) { //i = 1
        int k = i;
        for(int j = 1;j<=n-1;j++) {
            printf("%3d",k);
            k+=n;
        }
       printf("\n"); 
    }

    return 0;
}
