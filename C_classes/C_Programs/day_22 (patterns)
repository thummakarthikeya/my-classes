// Online C compiler to run C program online
#include <stdio.h>

int main() {
    int n=5;
    for(int i = 1;i<=n;i++) { //i = 3
    char ch = 'A'+n-i; 
       
        for(int j = 1;j<=n;j++) {
            printf("%3c",ch);
            ch+=n;
                                
        }
      printf("\n"); 
    }

   
    return 0;
}
