Ques - 9
-----------------------
#include <stdio.h>

int main() {
   int n=5,i,j;
    for(i = 1;i<=n;i++) {  
        for(j = 1;j<=n;j++) { 
            printf("%3d",(i+j)%2);  
        }                      
        printf("\n");
    }

    return 0;
}
==================================================
Ques - 10
-----------------------
#include <stdio.h>

int main() {
   int n=7,i,j;
    for(i = 1;i<=n;i++) {  
        for(j = 1;j<=n;j++) { 
            if(i==j || i+j == n+1) {
            printf("%3d",1);
            }else {
                printf("%3d",0);
            }
        }                      
        printf("\n");
    }

    return 0;
}
