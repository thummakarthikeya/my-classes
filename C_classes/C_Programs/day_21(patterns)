  1  7  2  7  3  7  4  7

#include <stdio.h>

int main() {
   int n=7,i,j;//11
    for(i = 1;i<=n;i++) {  // n = 5 times
        for(j = 1;j<=n/2+1;j++) { // j = 5/2 = 2 + 1 = 3
            printf("%3d%3d",j,i); 
        }
        printf("\n");
    }

    return 0;
}
