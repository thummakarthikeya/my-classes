#include <stdio.h>

int main() {
   int n=4,i,j,a =2;//11
    for(i = 1;i<=n;i++) {  // i = 2
        for(j = 1;j<=n+1;j++) { // j =1 2 3 4 5 
            printf("%3d",a); 
            a+=2;            
        }
        printf("\n");
    }

    return 0;
}

