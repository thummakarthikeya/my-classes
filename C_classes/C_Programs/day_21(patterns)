#include <stdio.h>

int main() {
   int n=4,i,j,a =1;
    for(i = 1;i<=n;i++) {
        for(j = 1;j<=n+1;j++) {
            printf("%3d",a);
            a+=2;
        }
        printf("\n");
    }

    return 0;
}

