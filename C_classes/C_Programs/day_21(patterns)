int main() {
   int n=4,i,j,a =1;
    for(i = 1;i<=n;i++) {
        for(j = 1;j<=n+1;j++) {
            printf("%3d",a++);
        }
        printf("\n");
    }
=========================
Ques - 2
-----------------------
#include <stdio.h>

int main() {
   int n=4,i,j,a =1;
    for(i = 1;i<=n;i++) {
        for(j = 1;j<=n+1;j++) {
            printf("%3d",a);
            a+=2;
        }
        printf("\n");
    }

    return 0;
}

==================================
Ques - 3
-----------------------
#include <stdio.h>

int main() {
   int n=4,i,j,a =2;//11
    for(i = 1;i<=n;i++) {  // i = 2
        for(j = 1;j<=n+1;j++) { // j =1 2 3 4 5 
            printf("%3d",a); 
            a+=2;            
        }
        printf("\n");
    }

    return 0;
}

=======================================
Ques - 4
-----------------------
#include <stdio.h>

int main() {
   int n=4,i,j;//11
    for(i = 1;i<=n;i++) {  // n = 4 times
        for(j = n+1;j>=1;j--) { // j =1 2 3 4 5 
            printf("%3d",j); 
        }
        printf("\n");
    }

    return 0;
}

==========================================
Ques - 5
-----------------------
#include <stdio.h>

int main() {
   int n=7,i,j;//11
    for(i = 1;i<=n;i++) {  // n = 5 times
        for(j = 1;j<=n/2+1;j++) { // j = 5/2 = 2 + 1 = 3
            printf("%3d%3d",j,i); 
        }
        printf("\n");
    }

    return 0;
}
==========================================
Ques - 6
-----------------------
#include <stdio.h>

int main() {
   int n=5,i,j;//11
    for(i = 1;i<=n;i++) {  // n = 5 times
        for(j = 1;j<=n;j++) { // j = 5/2 = 2 + 1 = 3
            printf("%3d",i*j); 
        }
        printf("\n");
    }

    return 0;
}
=====================================
Ques - 7
-----------------------
#include <stdio.h>

int main() {
   int n=5,i,j;//11
    for(i = 1;i<=n;i++) {  //  i =2
    int k = i; // 3
        for(j = 1;j<=n;j++) { //  5 times
            printf("%3d",k++); //1 2 3 4 5
        }                      //2 3 4 5 6 
        printf("\n");
    }

    return 0;
}
==========================================
Ques - 8
-----------------------
#include <stdio.h>

int main() {
   int n=5,i,j;//11
    for(i = n;i>=1;i--) {  //  i = 4
    int k = i; // 9
        for(j = 1;j<=n;j++) { 
            printf("%3d",k);  
            k+=n;             
        }                      
        printf("\n");
    }

    return 0;
}
