#include <stdio.h>

int main() {
   int n=5,i,j;//11
    for(i = 1;i<=n;i++) {  //  i =2
    int k = i; // 3
        for(j = 1;j<=n;j++) { //  5 times
            printf("%3d",k++); //1 2 3 4 5
        }                      //2 3 4 5 6 
        printf("\n");
    }

    return 0;
}
