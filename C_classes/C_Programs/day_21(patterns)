#include <stdio.h>

int main() {
   int n=5,i,j;//11
    for(i = 1;i<=n;i++) {  // n = 5 times
        for(j = 1;j<=n;j++) { // j = 5/2 = 2 + 1 = 3
            printf("%3d",i*j); 
        }
        printf("\n");
    }

    return 0;
}
