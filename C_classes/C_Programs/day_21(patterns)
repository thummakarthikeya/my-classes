#include <stdio.h>

int main() {
   int n=4,i,j;//11
    for(i = 1;i<=n;i++) {  // n = 4 times
        for(j = n+1;j>=1;j--) { // j =1 2 3 4 5 
            printf("%3d",j); 
        }
        printf("\n");
    }

    return 0;
}

