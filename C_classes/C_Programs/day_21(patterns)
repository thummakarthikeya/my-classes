#include <stdio.h>

int main() {
   int n=5,i,j;//11
    for(i = n;i>=1;i--) {  //  i = 4
    int k = i; // 9
        for(j = 1;j<=n;j++) { 
            printf("%3d",k);  
            k+=n;             
        }                      
        printf("\n");
    }

    return 0;
}
