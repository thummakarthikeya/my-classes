Ques - 1
=========================
// Online C compiler to run C program online
#include <stdio.h>

int main() {
    int n=5,k=1;
    for(int i = 1;i<=n;i++) {
        for(int j = 1;j<=n;j++) {
            if(i%2!=0) printf("%3d",k++);
            else printf("%3d",--k);
        }
        k+=n;
       printf("\n"); 
    }

    return 0;
}

