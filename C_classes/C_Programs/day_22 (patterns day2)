// Online C compiler to run C program online
#include <stdio.h>

int main() {
    int n=4,k=1;
    for(int i = 1;i<=n;i++) { 
        for(int j = 1;j<=n;j++) {
            if(j<=i) {
                printf("%3d",k++);
            } else {
                printf("%3c",' ');
            }         
        }
      printf("\n"); 
    }

   
    return 0;
}
