Ques - 9
=========================
Solution - 1
-------------------
#include <stdio.h>

int main() {
    int n=6;
    for(int i = 1;i<=n;i++) { 
        
        for(int j = 1;j<=n;j++) { //1 
            if(j<=i) {
                if(j==1 || i == n || i==j) {
                    printf("%3d",1);
                }else {
                    printf("%3d",j);
                }
               
            } else {
                printf("%3c",' ');
            }         
        }
      printf("\n"); 
    }

   
    return 0;
}

Solution - 2
-------------------------
// Online C compiler to run C program online
#include <stdio.h>

int main() {
    int n=6;
    for(int i = 1;i<=n;i++) { 
        
        for(int j = 1;j<=i;j++) { //1 
           
                if(j==1 || i == n || i==j) {
                    printf("%3d",1);
                }else {
                    printf("%3d",j);
                }
               
            }         
        
      printf("\n"); 
    }

   
    return 0;
}


