Ques - 4
=========================
// Online C compiler to run C program online
#include <stdio.h>

int main() {
    int n=5;
    for(int i = 1;i<=n;i++) { //i = 3
    char ch = 'A'+i-1; 
       
        for(int j = 1;j<=n;j++) {
            printf("%3c",ch++); //A B C D E
                                //B C D E F
        }
       printf("\n"); 
    }

    return 0;
}
