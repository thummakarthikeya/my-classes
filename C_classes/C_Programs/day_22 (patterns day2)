Ques - 6
=========================
int main() {
    int n=5;
    for(int i = 1;i<=n;i++) { 
        for(int j = 1;j<=n;j++) {
            if(j<=i) {
                printf("%3c",'*');
            } else {
                printf("%3c",' ');
            }         
        }
      printf("\n"); 
    }
